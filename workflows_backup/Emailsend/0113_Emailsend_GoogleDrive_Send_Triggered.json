  "connections": {
    "Google Drive Trigger": {
      "main": [
        [
          {
            "node": "Send Email",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "2ccb8cca-7919-4a9a-b1ea-4d887a170bfd": {
      "main": [
        [
          {
            "node": "error-handler-2ccb8cca-7919-4a9a-b1ea-4d887a170bfd-ebac6bf8",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "name": "Googledrivetrigger Workflow",
  "description": "Automated workflow: Googledrivetrigger Workflow. This workflow processes data and performs automated tasks.",
  "settings": {
