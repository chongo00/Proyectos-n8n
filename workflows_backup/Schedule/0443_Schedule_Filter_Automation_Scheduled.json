    {
      "id": "de421702-d012-4ea1-826e-1a4756ff4856",
      "name": "Get all emails in the last day",
      "type": "n8n-nodes-base.gmail",
      "position": [
        620,
        340
      ],
      "parameters": {
        "filters": {
          "q": "label:inbox",
          "receivedBefore": "={{ $now.minus({days: 1}) }}"
        },
        "resource": "thread",
        "returnAll": true
      },
      "credentials": {
        "gmailOAuth2": {
          "id": "{{ $credentials.gmailOAuth2.id }}",
          "name": "Work Gmail account"
        }
      },
      "retryOnFail": true,
      "typeVersion": 2
    },
