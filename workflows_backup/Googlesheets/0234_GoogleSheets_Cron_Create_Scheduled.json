      "name": "Google Sheets - read",
      "type": "n8n-nodes-base.googleSheets",
      "position": [
        300,
        160
      ],
      "parameters": {
        "options": {},
        "sheetId": "qwertz",
        "authentication": "{{ $credentials.oAuth2 }}"
      },
      "credentials": {
        "googleSheetsOAuth2Api": {
          "id": "{{ $credentials.googleSheetsOAuth2Api.id }}",
