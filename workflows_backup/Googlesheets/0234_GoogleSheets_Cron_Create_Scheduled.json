      "position": [
        100,
        160
      ],
      "parameters": {
        "triggerTimes": {
          "item": [
            {
              "hour": 5,
              "mode": "everyWeek"
            }
          ]
        }
      },
      "typeVersion": 1,
      "id": "dbfc88c9-36e2-4cce-b661-e6767fcae129"
    },
