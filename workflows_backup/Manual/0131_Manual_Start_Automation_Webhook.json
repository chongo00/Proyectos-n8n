        "sendBinaryData": true,
        "binaryPropertyName": "banner:data"
      },
      "credentials": {
        "oAuth1Api": {
          "id": "{{ $credentials.oAuth1Api.id }}",
          "name": "Unnamed credential"
        }
      },
      "typeVersion": 1
    }
  ],
  "connections": {
    "HTTP Request": {
      "main": [
        [
          {
            "node": "HTTP Request1",
            "type": "main",
            "index": 0
          }
